//C++ -->

int linearSearch(int n, int num, vector<int> &arr)
{
    for(int i=0;i<num;i++){
        if(arr[i]==num){
            return i;
        }
    }
    return -1;
}

//Java -->

import java.util.*;
public class Solution {
    public static int linearSearch(int n, int num, int []arr){
        for(int i=0; i<n; i++){
            if (arr[i] == num){
                return i;
            }
        }
        return -1;
    }
}
